{
    "manifest_version": 2,
    "name": "Simple extension",
    "version": "0.1",

        "content_scripts": [
            {
                "matches": [
                    "https://www.youtube.com/*"
                ],
                "js": ["content.js"],
                "run_at": "document_start"

            }
        ],

        "background" : {
            "scripts" : ["background.js"]
        },

        "permissions": [
            "https://www.youtube.com/*", "tabs", "webNavigation"
        ]

}



